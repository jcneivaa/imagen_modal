{
    "name": "unal/imagen-modal",
    "type": "typo3-cms-extension",
    "description": "Modal para desplegar una imagen al entrar en una pagina.",
    "authors": [
        {
            "name": "Camilo Neiva",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7.1"
    },
    "autoload": {
        "psr-4": {
            "UNAL\\ImagenModal\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UNAL\\ImagenModal\\Tests\\": "Tests"
        }
    },
    "replace": {
        "imagen_modal": "self.version",
        "typo3-ter/imagen-modal": "self.version"
    }
}